{
  "name": "rembrandt-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "nodemon",
    "lint": "tslint -p tsconfig.json",
    "development": "npm run lint && node --inspect=9229 -r tsconfig-paths/register -r ts-node/register src/app.ts",
    "build": "tsc && cp src/swagger.json build/swagger.json",
    "production": "node -r tsconfig-paths/register build/app.js",
    "swagger": "swagger-jsdoc -d swaggerConf.js -o src/swagger.json"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "jsonapi-serializer": "^3.6.4",
    "mongoose": "^5.5.5",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.0.2",
    "tsconfig-paths": "^3.8.0",
    "tslib": "^1.9.3",
    "typegoose": "^5.6.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.5",
    "@types/express": "^4.16.1",
    "@types/mongoose": "^5.3.27",
    "@types/swagger-ui-express": "^3.0.0",
    "@types/jsonapi-serializer": "^3.6.1",
    "nodemon": "^1.18.11",
    "swagger-jsdoc": "^3.2.9",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  }
}
